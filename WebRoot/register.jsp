                <div class="login-reg-form">
                    <form action="userRegister"  method="post">
                        <div class="row">
                            <div class="col-sm-6 col-xs-12 mb-20">
                                <label for="r_f_name">用&nbsp;户&nbsp;名<span class="required">&nbsp;*</span></label>										
                                <input id="r_f_name" type="text" name="username"/>
                            </div>
                           <div class="col-sm-6 col-xs-12 mb-20">
                                <label for="r_email">邮&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;箱 <span class="required">*</span></label>										
                                <input id="r_email" type="email" name="email"/>
                            </div>
                            <div class="col-sm-6 col-xs-12 mb-20">
                                <label for="r_phone">电&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;话  <span class="required">*</span></label>										
                                <input id="r_phone" type="text" name="phone"/>
                            </div>
                           
                            <div class="col-xs-12 mb-20">
                                <label>地&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;址 <span class="required">*</span></label>
                                <input type="text" placeholder="Street address" name="address" />
                            </div>
                            <div class="col-xs-12 mb-20">
                                <label class="" for="r_password">密&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;码<span class="required">&nbsp;*</span></label>
                                <input id="r_password" type="password" name=password>
                            </div>
                            <div class="col-xs-12 mb-20">
                                <label class="" for="r_c_password">确认密码<span class="required">*</span></label>
                                <input id="r_c_password" type="password" name="password1">
                            </div>
                            <div class="col-xs-12 mb-20">
                                <input id="rememberme" type="checkbox">
                                <label for="rememberme">我同意该 <a href="#">用户协议</a></label>
                            </div>
                            <div class="col-xs-12">
                                <input value="注册" type="submit">
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
